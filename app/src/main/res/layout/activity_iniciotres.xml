<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/iniciotres"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/iniciotres"
    tools:context=".MainActivity">

    <Button

        android:id="@+id/btn_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="33dp"
        android:layout_marginBottom="19dp"
        android:onClick="inicioquatro"
        android:text="@string/proximo"
        android:backgroundTint="#430000"
        android:visibility="visible"
        tools:visibility="visible" />

    <Button

        android:id="@+id/back2"

        android:layout_width="108dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="57dp"
        android:layout_marginEnd="246dp"
        android:layout_marginBottom="19dp"
        android:backgroundTint="#430000"
        android:text="@string/voltar"
        android:visibility="visible"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="349dp"
        android:layout_height="51dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="31dp"
        android:layout_marginTop="33dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="575dp"
        android:gravity="center"
        android:text="@string/avafast"
        android:textAllCaps="true"
        android:textColor="#FFFFFF"
        android:textSize="34sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="351dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="175dp"
        android:gravity="center"
        android:text="@string/como_funcionamos"
        android:textAllCaps="true"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="377dp"
        android:layout_height="73dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="17dp"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="94dp"
        android:gravity="center"
        android:text="@string/o_intuiro_do_nosso_aplicativo_oferecer_avalia_o_para_os_restaurantes_em_troca_de_recompensas_para_seus_clientes"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        tools:ignore="TextSizeCheck,VisualLintBounds" />

</RelativeLayout>