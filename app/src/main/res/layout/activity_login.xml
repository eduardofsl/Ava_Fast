<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/login"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/login"
    tools:context=".login">

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="31dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="23dp"
        android:layout_marginTop="383dp"
        android:layout_marginEnd="257dp"
        android:layout_marginBottom="242dp"
        android:gravity="center"
        android:text="@string/entre_na_sua_conta"
        android:textColor="#FFFFFF"
        android:textSize="15sp"
        android:textStyle="bold"
        tools:ignore="VisualLintOverlap" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="125dp"
        android:layout_marginBottom="269dp"
        android:autofillHints=""
        android:ems="10"
        android:inputType="textEmailAddress"
        android:minHeight="48dp"
        android:text="@string/endere_o_de_e_mail"
        android:textColor="#FFFFFF"
        tools:ignore="LabelFor,VisualLintOverlap" />

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="126dp"
        android:layout_marginBottom="207dp"
        android:autofillHints=""
        android:ems="10"
        android:inputType="textPassword"
        android:minHeight="48dp"
        android:text="@string/insira_sua_senha"
        android:textColor="#FFFFFF"
        tools:ignore="LabelFor,VisualLintOverlap" />

    <Button

        android:id="@+id/back3"
        android:layout_width="186dp"
        android:layout_height="89dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="17dp"
        android:layout_marginTop="744dp"
        android:layout_marginEnd="208dp"
        android:layout_marginBottom="33dp"
        android:backgroundTint="#430000"
        android:text="@string/voltar"
        android:textSize="20sp"
        android:visibility="visible"
        tools:ignore="VisualLintButtonSize,VisualLintOverlap"
        tools:visibility="visible" />

    <Switch
        android:id="@+id/switch1"
        android:layout_width="157dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="237dp"
        android:layout_marginBottom="136dp"
        android:minHeight="48dp"
        android:text="@string/lembrar_sempre"
        android:textColor="#FFFFFF"
        tools:ignore="UseSwitchCompatOrMaterialXml,VisualLintOverlap" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="346dp"
        android:layout_marginBottom="275dp"
        android:contentDescription="@string/todo"
        app:srcCompat="@android:drawable/ic_dialog_email" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="44dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="346dp"
        android:layout_marginBottom="210dp"
        android:contentDescription="@string/todo"
        app:srcCompat="@android:drawable/ic_lock_lock"
        app:tint="#FFFFFF"
        tools:ignore="ImageContrastCheck" />

    <Button
        android:id="@+id/button"
        android:layout_width="186dp"
        android:layout_height="89dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="218dp"
        android:layout_marginTop="744dp"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="33dp"
        android:backgroundTint="#430000"
        android:text="@string/login"
        android:textAllCaps="false"
        android:textSize="20sp"
        android:visibility="visible"
        tools:ignore="VisualLintButtonSize"
        tools:visibility="visible" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="216dp"
        android:contentDescription="@string/todo"
        app:srcCompat="@android:drawable/ic_menu_view"
        app:tint="#FFFFFF" />
</RelativeLayout>